{
  "name": "CampusGuide",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "flow": "flow .",
    "lint": "eslint .",
    "react-native:ios": "react-native bundle --entry-file index.ios.js --bundle-output ios/main.jsbundle --platform ios --dev false",
    "react-native:android": "react-native bundle --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --platform android --dev false",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --coverage"
  },
  "dependencies": {
    "react": "15.0.2",
    "react-native": "^0.26.2",
    "react-native-maps": "^0.4.2",
    "react-native-orientation": "^1.16.0",
    "react-native-vector-icons": "^2.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-jest": "^12.1.0",
    "babel-polyfill": "^6.9.1",
    "eslint": "^2.11.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-react-native": "^1.0.2",
    "jest-cli": "^12.1.1",
    "rnpm-plugin-upgrade": "^0.26.0"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "haste": {
      "defaultPlatform": "ios",
      "platforms": [
        "ios",
        "android"
      ],
      "providesModuleNodeModules": [
        "react-native"
      ]
    }
  }
}
