  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "moduleNameMapper": {
      "^[./]*assets[a-zA-Z0-9./]*/([a-zA-Z.]+)$": "empty/object"
