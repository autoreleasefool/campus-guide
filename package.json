{
  "name": "CampusGuide",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "flow": "flow .",
    "lint": "eslint .",
    "build:ios": "react-native bundle --entry-file index.ios.js --bundle-output ios/main.jsbundle --platform ios --dev false",
    "build:android": "react-native bundle --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --platform android --dev false",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --coverage"
  },
  "dependencies": {
    "react": "^15.1.0",
    "react-native": "0.27.0",
    "react-native-maps": "^0.4.2",
    "react-native-orientation": "^1.16.0",
    "react-native-vector-icons": "^2.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "babel-jest": "^13.0.0",
    "babel-polyfill": "^6.9.1",
    "empty": "^0.10.1",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-react-native": "^1.1.0",
    "flow-bin": "^0.25.0",
    "jest-cli": "^13.0.0",
    "rnpm-plugin-upgrade": "^0.26.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "globals": {
      "__DEV__": true
    },
    "haste": {
      "defaultPlatform": "ios",
      "platforms": [
        "ios",
        "android"
      ],
      "providesModuleNodeModules": [
        "react-native"
      ]
    },
    "moduleNameMapper": {
      "^[./]*assets[a-zA-Z0-9./]*/([a-zA-Z.]+)$": "empty/object"
    }
  }
}
